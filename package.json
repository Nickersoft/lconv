{
  "name": "lconv",
  "version": "0.0.2",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "import": "node --es-module-specifier-resolution=node --loader ts-node/esm src/importer/import.ts",
    "build": "concurrently \"yarn:tsc\" \"yarn:esbuild --sourcemap --target=node16 --platform=node --format=esm --outdir=dist src/index.ts\"",
    "postinstall": "yarn import",
    "prepublish": "yarn build",
    "postpublish": "rimraf dist && git push --follow-tags",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.5.9",
    "csv": "^6.1.0",
    "lodash": "^4.17.11",
    "node-fetch": "^3.2.4",
    "typescript": "^4.7.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.36",
    "concurrently": "^7.2.1",
    "esbuild": "^0.14.42",
    "jest": "^28.1.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.8.0"
  },
  "packageManager": "yarn@3.2.1"
}
